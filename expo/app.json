{
	"expo": {
		"name": "Cronch",
		"description": "The all-in-one app to manage your time",
		"slug": "typescript",
		"privacy": "public",
		"sdkVersion": "31.0.0",
		"platforms": ["ios", "android"],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#F7F7F7"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true
		}
	}
}
